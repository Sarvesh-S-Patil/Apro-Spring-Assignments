package com.apro.main.entity;

import java.time.LocalDate;

import jakarta.persistence.Entity;
import jakarta.persistence.Table;

@Entity
@Table(name="employee")
public class Employee {
	long employeeId;
	String firstName;
	String lastName;
	long phoneNumber;
	String email;
	String position;
	LocalDate hireDate;
	double salary;
	long accountNumber;
	EmployeeStatus status;

}
